{
  "name": "Demo",
  "version": "0.1",
  "description": "Hello World Gmail chrome extension",
  "content_scripts": [
    {
      "matches": ["https://mail.google.com/*"],
      "js": [
        "js/jquery-1.10.2.min.js",
        // "js/gmail.js",
        // "js/main.js"
        "js/content.js"
      ],
      "css": [
        "css/content.css"
      ]
    }
  ],
  "permissions": [
    "storage"
  ],
  "background": {
    "scripts": [
      "js/jquery-1.10.2.min.js",
      "js/background.js"
    ]
  },
  "web_accessible_resources": [
    "js/jquery-1.10.2.min.js",
    "js/gmail.js",
    "js/main.js",
    "images/toggle.png"
  ],
  "manifest_version": 2
}
